pipeline {
    agent any
    stages {

        stage('pull') {
            steps {
                git branch: 'master', url:'https://github.com/chandra24arch/Amazon.git'
            }
        }
        stage('compile') {
            steps {
                sh 'mvn compile'
            }
        }

        stage('build') {
            steps {
                 sh 'mvn clean install'
            }
        }

        
    }

  post{

  success{
     echo 'Build success'
  }
    
  failure{
       echo 'Failure in the build'
   }

  }


}
